{
  "private": true,
  "scripts": {
    "build": "tsc && resolve-tspaths",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register source/index.ts",
    "prepare": "if [ \"$NODE_ENV\" != \"development\" ]; then exit 0; fi; husky install",
    "start": "NODE_ENV=production node build/index.js"
  },
  "lint-staged": {
    "**/*.(ts|js)": [
      "bash -c 'pnpm tsc --noEmit'",
      "bash -c 'pnpm prettier --write'"
    ]
  },
  "dependencies": {
    "discord.js": "^14.6.0",
    "discordx": "^11.4.0",
    "dotenv": "^16.0.3",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "resolve-tspaths": "^0.8.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=15.14.0",
    "pnpm": ">=7.12.2"
  }
}
